{
  "name": "split-cucumber",
  "version": "1.0.0",
  "description": "Run Cypress tests in parallel for free for Cucumber feature specs",
  "main": "index.js",
  "private": true,
  "scripts": {
    "test": "cypress run",
    "badges": "npx -p dependency-version-badge update-badge cypress cypress-split",
    "debug": "DEBUG=cypress-split SPLIT=2 SPLIT_INDEX=0 npx cypress run"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/e2e/step_definitions"
  },
  "keywords": [
    "cypress-split",
    "cypress-example"
  ],
  "author": "Gleb Bahmutov",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^13.6.6",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-split": "^1.21.0",
    "prettier": "^2.8.4"
  }
}
